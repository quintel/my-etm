<%= form_with(model: saved_scenario) do |form| %>
  <% if saved_scenario.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(saved_scenario.errors.count, "error") %> prohibited this saved_scenario from being saved:</h2>

      <ul>
        <% saved_scenario.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :scenario_id, style: "display: block" %>
    <%= form.number_field :scenario_id %>
  </div>

  <div>
    <%= form.label :scenario_id_history, style: "display: block" %>
    <%= form.text_area :scenario_id_history %>
  </div>

  <div>
    <%= form.label :title, style: "display: block" %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :description, style: "display: block" %>
    <%= form.text_area :description %>
  </div>

  <div>
    <%= form.label :area_code, style: "display: block" %>
    <%= form.text_field :area_code %>
  </div>

  <div>
    <%= form.label :end_year, style: "display: block" %>
    <%= form.number_field :end_year %>
  </div>

  <div>
    <%= form.label :private, style: "display: block" %>
    <%= form.check_box :private %>
  </div>

  <div>
    <%= form.label :created_at, style: "display: block" %>
    <%= form.datetime_field :created_at %>
  </div>

  <div>
    <%= form.label :updated_at, style: "display: block" %>
    <%= form.datetime_field :updated_at %>
  </div>

  <div>
    <%= form.label :discarded_at, style: "display: block" %>
    <%= form.datetime_field :discarded_at %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
