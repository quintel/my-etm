<div class="flex">
  <div class="flex flex-col">
    <div>
      <span><%= @saved_scenario.area_code%></span>
      <span><%= @saved_scenario.end_year%></span>
    </div>
    <span class="text-sm text-midnight-400"><%= "Last updated #{@time} ago"%></span>
  </div>
  <% if !@saved_scenario.discarded? %>
    <%= link_to @path, class: "button mr-0 ml-auto" do %>
      <%= @button_title %>
    <% end %>
  <% end %>
</div>
